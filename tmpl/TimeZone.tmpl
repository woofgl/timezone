<script id="tmpl-TimeZone" type="text/html">
    <div>
        {{for timeZone}}
        <div class="timeZone">
            <div>{{:Dcity}}, {{:Dcounty}}</div>
            <div>{{:Dhour}}:{{:Dminute}}:{{:Dsecond}}</div>
            <div>{{:Dweek}}, {{:Dmonth}}/{{:Dday}}/{{:Dyear}}</div>
        </div>
        <div class="clockTime"></div>
        {{/for}}
    </div>

</script>